{
  "name": "my-graphql-mock",
  "version": "1.0.0",
  "description": "My test of the Apollo graphql server tools for mocking",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel webpack test:watch serve:bundle",
    "webpack": "webpack --watch -d",
    "serve:bundle": "nodemon dist/backend.js",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chookie/graphql-mock.git"
  },
  "keywords": [
    "apollo",
    "graphql",
    "server-tools",
    "react"
  ],
  "author": "Alison Johnston",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chookie/graphql-mock/issues"
  },
  "homepage": "https://github.com/chookie/graphql-mock#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "body-parser": "^1.16.0",
    "bunyan": "^1.8.5",
    "bunyan-prettystream": "^0.1.3",
    "casual-browserify": "^1.5.8",
    "config": "^1.25.1",
    "express": "^4.14.0",
    "express-graphql": "^0.6.1",
    "graphql": "^0.8.2",
    "graphql-tools": "^0.8.4",
    "http": "0.0.0",
    "mongodb": "^2.2.16",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "source-map-support": "^0.4.11",
    "webpack": "1.14.0"
  }
}
